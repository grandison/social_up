<script>
  $(function(){
    VK.api('audio.search', {q: "Хуй"}, function(r) { 
      if(r.response) {
        $.each(r.response, function( index, value ){
          if(index != 0) {
            var item = $("<p></p>").text(value["title"]);
            $("#music-list").append(item);
          };
        });
      };
    }); 
  });
</script>
